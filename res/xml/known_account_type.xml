<?xml version="1.0" encoding="UTF-8"?>
<!-- id : identity of the account type, for custom login, will use domain as the id
	icon : icon resource id in res/drawable folder
	name : name resource id in strings.xml
	domain : when needSrv == 1, domain is used as DNS SRV, otherwise it is the xmpp server
	port : xmpp port
	ssl : support ssl
	needsrv : use DNS SRV
	description : input description for user
 -->
<known-account-type>    
	<account-type id="cisco jabber" icon="cisco" name="cisco" domain="cisco.com" port="5222" ssl="1" needsrv="1" description="username@cisco.com"/>
	<account-type id="gtalk" icon="gtalk" name="gtalk" domain="gmail.com" port="5222" ssl="1" needsrv="1" description="username@gmail.com"/>
	<account-type id="weibo" icon="weibo" name="weibo" domain="xmpp.weibo.com" port="5222" ssl="1" needsrv="0" description="weibo account name"/>
	<account-type id="renrern" icon="renren" name="renren" domain="talk.renren.com" port="5222" ssl="1" needsrv="0" description="renren account name"/>
</known-account-type>